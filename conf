server {
    listen 80;
    server_name upcard.online;

    location /static/ {
        alias /var/www/upcardapi/static/;
    }

    location /media/ {
        alias /var/www/upcardapi/media/;
    }

    location / {
        uwsgi_pass unix:///var/www/upcardapi/upcardapi;
        include uwsgi_params;
    }
}